{
    "title": "Input schema for the Parsera Actor",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "url": {
            "title": "URL",
            "type": "string",
            "description": "URL of the page to scrape",
            "editor": "textfield"
        },
        "apiKey": {
            "title": "Parsera API Key",
            "type": "string",
            "description": "Your Parsera API key",
            "editor": "textfield"
        },
        "attributes": {
            "title": "Attributes to extract",
            "type": "array",
            "description": "List of attributes to extract from the page",
            "editor": "json",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "title": "Attribute name",
                        "type": "string",
                        "description": "Name of the attribute"
                    },
                    "description": {
                        "title": "Attribute description",
                        "type": "string",
                        "description": "Description of what to extract"
                    }
                },
                "required": ["name", "description"]
            }
        }
    },
    "required": ["url", "apiKey", "attributes"]
}
